async def sub_points_to_member(user_id: str, points: int): 
    try:     
        user_id = str(user_id)
        points = int(points)
        with open(PATH_TO_USERS_FILE, "r" ) as users_file:
            data = json.load(users_file)
        if not user_id in data:
            return {"status" : False, "error" : "user not found"}
        balance = data[user_id].get("score", "Not_found")
        if balance == "Not_found":
            return {"status" : False, "error" : "score not found in user.json"}
        balance -= int(points)
        if balance < 0:
            balance = 0
        data[user_id]["score"] = balance
        with  open(PATH_TO_USERS_FILE, "w" ) as users_file:
            json.dump(data, users_file, ensure_ascii=False, indent = 4)
        return {"status" : True, "error" : "success"}
    except ValueError:
        return {"status" : False, "error" : "ValueError send correct values"}
